{
  "AllowedHosts": "*",

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {

    "OMDB": "Server=[YOUR_SERVER_NAME];Database=[YOUR_DATABASE_NAME];User Id=[YOUR_USERNAME];Password=[YOUR_PASSWORD];TrustServerCertificate=true;Encrypt=true"
  },

  "DatabaseSeeding": {
    "Enabled": true,
    "SeedCustomers": true,
    "SeedSampleOrders": false,
    "Environment": "Development"
  }


}
